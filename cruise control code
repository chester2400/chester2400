  submenu1 = 4;
  }
  if (submenu1 == 1){
    submenu1 = 7;
  }
}
}
if ((menu == 20) && (submenu1 >= 2) && (submenu1 <=7)){        // ustawianie proportional low i high
if (spA == 1){
  if (submenu1 == 2){
  Kpl = Kpl + 0.01;
  spA = 0;
  }
  if (submenu1 == 3){
  Kil = Kil + 0.01;
  spA = 0;  
  }
  if (submenu1 == 4){
    Kdl = Kdl + 0.01;
    spA = 0;
    }
         if (submenu1 == 5){
         Kph = Kph + 0.01;
          spA = 0;
         }
        if (submenu1 == 6){
        Kih = Kih + 0.01;
        spA = 0;  
        }
